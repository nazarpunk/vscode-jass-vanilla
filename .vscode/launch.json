{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Client",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceRoot}"
      ],
      "outFiles": [
        "${workspaceRoot}/client/out/**/*.js"
      ],
      "autoAttachChildProcesses": true,
      "preLaunchTask": {
        "type": "npm",
        "script": "compile"
      }
    }
  ]
}
