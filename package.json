{
  "version": "0.0.1",
  "license": "MIT",
  "author": "nazarpunk",
  "publisher": "nazarpunk",
  "name": "jass-vanilla",
  "displayName": "jass-vanilla",
  "contributors": [],
  "description": "Vanilla JASS syntax.",
  "private": false,
  "keywords": [
    "jass"
  ],
  "categories": [
    "Programming Languages",
    "Snippets",
    "Formatters"
  ],
  "activationEvents": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/nazarpunk/vscode-jass-vanilla"
  },
  "main": "./out/extension.js",
  "scripts": {
    "esbuild-chevrotain-calculator": "esbuild docs/calculator/main.mjs --outfile=docs/calculator/out/main.mjs --target=es2020 --bundle --external:vscode --format=cjs --platform=browser --tree-shaking=true --sourcemap --watch",
    "esbuild-chevrotain": "esbuild docs/main.mjs --outfile=docs/out/main.mjs --target=es2020 --bundle --external:vscode --format=cjs --platform=browser --tree-shaking=true --sourcemap --watch",
    "vscode:prepublish": "npm run esbuild-base",
    "build": "vsce package",
    "publish": "vsce publish patch",
    "esbuild-base": "esbuild src/extension.mjs --outfile=out/extension.js --target=es2020 --bundle --external:vscode --format=cjs --platform=node --tree-shaking=true --sourcemap",
    "esbuild": "npm run esbuild-base",
    "esbuild-watch": "npm run esbuild-base -- --watch"
  },
  "editor.semanticHighlighting.enabled": true,
  "contributes": {
    "semanticTokenTypes": [
      {
        "id": "jass_comment",
        "description": "jass_comment"
      },
      {
        "id": "jass_type",
        "description": "jass_type"
      },
      {
        "id": "jass_function",
        "description": "jass_function"
      },
      {
        "id": "jass_function_native",
        "description": "jass_function_native"
      },
      {
        "id": "jass_argument",
        "description": "jass_argument"
      },
      {
        "id": "jass_comma",
        "description": "jass_comma"
      },
      {
        "id": "jass_type_keyword",
        "description": "jass_type_keyword"
      },
      {
        "id": "jass_extends_keyword",
        "description": "jass_extends_keyword"
      },
      {
        "id": "jass_constant_keyword",
        "description": "jass_constant_keyword"
      },
      {
        "id": "jass_native_keyword",
        "description": "jass_native_keyword"
      },
      {
        "id": "jass_function_keyword",
        "description": "jass_function_keyword"
      },
      {
        "id": "jass_endfunction_keyword",
        "description": "jass_endfunction_keyword"
      },
      {
        "id": "jass_takes_keyword",
        "description": "jass_takes_keyword"
      },
      {
        "id": "jass_returns_keyword",
        "description": "jass_returns_keyword"
      }
    ],
    "configurationDefaults": {
      "editor.semanticTokenColorCustomizations": {
        "rules": {
          "jass_comment": "#308030",
          "jass_type": "#4EC9B0",
          "jass_function": "#DCDCAA",
          "jass_function_native": "#C586C0",
          "jass_argument": "#9CDCDA",
          "jass_comma": "#FFFFFF",
          "jass_type_keyword": "#2C7AD6",
          "jass_extends_keyword": "#2C7AD6",
          "jass_constant_keyword": "#2C7AD6",
          "jass_native_keyword": "#2C7AD6",
          "jass_function_keyword": "#2C7AD6",
          "jass_endfunction_keyword": "#2C7AD6",
          "jass_takes_keyword": "#2C7AD6",
          "jass_returns_keyword": "#2C7AD6"
        }
      }
    },
    "languages": [
      {
        "id": "jass",
        "aliases": [
          "JASS",
          "jass"
        ],
        "extensions": [
          "j"
        ],
        "configuration": "./syntaxes/jass.language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "jass",
        "scopeName": "source.jass",
        "path": "./syntaxes/jass.tmLanguage.json"
      }
    ]
  },
  "configurationDefaults": {
    "jass": {
      "editor.semanticHighlighting.enabled": true
    }
  },
  "devDependencies": {
    "chevrotain": "^11.0.1",
    "esbuild": "^0.18.14",
    "@vscode/vsce": "^2.15.0",
    "@types/vscode": "^1.80.0"
  },
  "engines": {
    "vscode": "^1.80.0"
  }
}