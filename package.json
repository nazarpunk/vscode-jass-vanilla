  "name": "jass-vanilla",
  "displayName": "jass-vanilla",
  "contributors": [],
  "description": "Vanilla JASS syntax.",
  "private": false,
  "icon": "images/logo.png",
  "keywords": [
    "jass",
    "warcraft",
    ".wts"
  ],
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets",
    "Formatters"
  ],
  "activationEvents": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/nazarpunk/vscode-jass-vanilla"
  },
  "bugs": {
    "url": "https://github.com/nazarpunk/vscode-jass-vanilla/issues"
  },
  "main": "./out/extension.js",
  "scripts": {
    "esbuild-docs-calculator": "esbuild docs/calculator/main.mjs --outfile=docs/calculator/out/main.mjs --target=es2020 --bundle --external:vscode --format=cjs --platform=browser --tree-shaking=true --sourcemap --watch",
    "esbuild-docs-wts": "esbuild docs/wts/main.mjs --outfile=docs/wts/out/main.mjs --target=es2020 --bundle --external:vscode --format=cjs --platform=browser --tree-shaking=true --sourcemap --watch",
    "esbuild-docs-jass": "esbuild docs/main.mjs --outfile=docs/out/main.mjs --target=es2020 --bundle --external:vscode --format=cjs --platform=browser --tree-shaking=true --sourcemap --watch",
    "esbuild-base": "esbuild src/extension.mjs --outfile=out/extension.js --target=es2020 --bundle --external:vscode --format=cjs --platform=node --tree-shaking=true --sourcemap",
    "esbuild": "npm run esbuild-base",
    "vscode:prepublish": "npm run esbuild-base",
    "build": "vsce package",
    "publish": "vsce publish patch"
  },
  "devDependencies": {
    "chevrotain": "^11.0.1",
    "esbuild": "^0.18.14",
    "@vscode/vsce": "^2.15.0",
    "@types/vscode": "^1.80.0"
  },
  "configurationDefaults": {
    "jass": {
      "editor.semanticHighlighting.enabled": true
    }
  },
  "contributes": {
    "languages": [
      {
        "id": "jass",
        "aliases": [
          "JASS",
          "jass"
        ],
        "extensions": [
          "j"
        ],
        "configuration": "./src/jass/syntaxes/language-configuration.json"
      },
      {
        "id": "wts",
        "aliases": [],
        "extensions": [
          "wts"
        ],
        "configuration": "./src/wts/syntaxes/language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "jass",
        "scopeName": "source.jass",
        "path": "./src/jass/syntaxes/tmLanguage.json"
      },
      {
        "language": "wts",
        "scopeName": "source.wts",
        "path": "./src/wts/syntaxes/tmLanguage.json"
      }
    ],
